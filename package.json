{
  "name": "muzykant-soundcloud-downloader",
  "version": "2.6.0",
  "description": "Download Soundcloud audio with Node.js",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "docs": "typedoc && mkdir -p ./docs && touch ./docs/.nojekyll",
    "build": "tsdown src/index.ts --format esm,cjs --dts --minify --clean",
    "publish-script": "./publish.sh",
    "test": "jest --runInBand"
  },
  "keywords": [
    "soundcloud",
    "soundcloud-api",
    "soundcloud downloader",
    "soundcloud download",
    "soundcloud audio",
    "download soundcloud",
    "scdl"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Muzykant-Team/node-soundcloud-downloader.git"
  },
  "author": "Zack Radisic",
  "maintainers": [
    "Muzykant Team"
  ],
  "contributors": [
    "Muzykant Team"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=24.13.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "axios": "^1.13.2",
    "dotenv": "^17.2.2",
    "m3u8stream": "^0.8.6",
    "soundcloud-key-fetch": "^1.0.13",
    "tsdown": "0.15.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@farmfe/core": "^1.7.11",
    "@farmfe/runtime": "^0.12.10",
    "@rspack/core": "^1.5.5",
    "@types/koa__cors": "^5.0.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-watch": "^7.8.1",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "file-type": "^21.0.0",
    "jest": "^30.1.3",
    "music-metadata": "^11.8.3",
    "pkg-types": "^2.3.0",
    "publint": "^0.3.13",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.2",
    "unloader": "^0.8.0",
    "unplugin-unused": "^0.5.3",
    "vitest": "^4.0.0",
    "webpack": "^5.101.3"
  }
}