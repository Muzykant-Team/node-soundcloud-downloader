{
  "name": "muzykant-soundcloud-downloader",
  "version": "1.0.0",
  "description": "Download Soundcloud audio with Node.js",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "docs": "typedoc --out docs ./src --excludeInternal && touch ./docs/.nojekyll",
    "build": "tsdown src/index.ts --format esm,cjs --dts --minify --clean",
    "publish-script": "./publish.sh",
    "test": "jest --runInBand"
  },
  "keywords": [
    "soundcloud",
    "soundcloud-api",
    "soundcloud downloader",
    "soundcloud download",
    "soundcloud audio",
    "download soundcloud",
    "scdl"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykPatryk5/node-soundcloud-downloader.git"
  },
  "author": "Zack Radisic",
  "maintainers": ["Muzykant Team"],
  "contributors": ["Muzykant Team"],
  "license": "MIT",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "axios": "^1.12.1",
    "dotenv": "^17.2.2",
    "m3u8stream": "^0.8.6",
    "tsdown": "^0.14.1",
    "soundcloud-key-fetch": "^1.0.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-watch": "^7.8.1",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "file-type": "^21.0.0",
    "jest": "^30.1.3",
    "music-metadata": "^11.8.3",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2"
  }
}
